{
  "name": "eventmapjs",
  "version": "0.1.3",
  "description": "ActionMap.js - Front Controller library following Flux Patter",
  "main": "lib/lib.js",
  "scripts": {
    "test": "node_modules/.bin/mocha ./**/*.spec.js --require babel-register",
    "lint": "node_modules/.bin/eslint ./**/*.js",
    "ci": "node_modules/.bin/mocha ./**/*.spec.js --require babel-register --reporter mocha-junit-reporter",
    "clean": "rimraf lib",
    "build": "npm run clean && rollup -c --config rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nirth/eventmapjs.git"
  },
  "keywords": [
    "fp",
    "flux",
    "store",
    "front-controller"
  ],
  "author": "David Sergey <me@davidsergey.com> (https://davidsergey.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nirth/eventmapjs/issues"
  },
  "homepage": "https://github.com/nirth/eventmapjs#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "dirty-chai": "^2.0.0",
    "eslint": "^4.1.1",
    "eslint-config-react-app": "^1.0.4",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "mocha": "^3.4.2",
    "mocha-circleci-reporter": "0.0.2",
    "mocha-junit-reporter": "^1.13.0",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "sinon": "^2.3.5",
    "sinon-chai": "^2.11.0",
    "sinon-chai-in-order": "^0.1.0"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "ramda": "^0.24.1",
    "rxjs": "^5.4.1"
