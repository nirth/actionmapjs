{
  "name": "matejs",
  "version": "0.4.0",
  "description": "Mate.js - Front Controller library following Flux Pattern",
  "main": "./lib/lib.js",
  "scripts": {
    "test": "yarn run typecheck && yarn run bdd",
    "bdd": "node_modules/.bin/mocha ./**/*.spec.js --require @babel/register",
    "ci": "node_modules/.bin/mocha ./**/*.spec.js --require @babel/register --reporter mocha-junit-reporter",
    "typecheck": "node_modules/.bin/flow",
    "clean": "rimraf lib",
    "build": "yarn run clean && rollup -c --config rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nirth/matejs.git"
  },
  "keywords": [
    "fp",
    "flux",
    "store",
    "front-controller"
  ],
  "author": "David Sergey Grigoryan <me@davidsergey.com> (https://davidsergey.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nirth/matejs/issues"
  },
  "homepage": "https://github.com/nirth/matejs#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "dirty-chai": "^2.0.1",
    "flow-bin": "^0.83.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-terser": "^3.0.0",
    "sinon": "^7.0.0",
    "sinon-chai": "^3.2.0",
    "sinon-chai-in-order": "^0.1.0",
    "terser": "^3.10.1"
  },
  "dependencies": {
    "ramda": "^0.25.0",
    "rxjs": "^6.3.3"
  }
}
